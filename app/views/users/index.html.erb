<% @users.each do |u| %>
  <%= u.username %>
  <br>
  <%= u.email %>
  <% u.addresses.each do |addy| %>
    <%= addy.street %>
    <br>
    <%= addy.city %>
    <br>
    <%= addy.state %>
    <br>
    <%= addy.country %>
    <br>
  <%end%>
  <%= link_to "Edit", edit_user_path(u) %>
  <%= link_to "Delete", user_path(u), :method => :delete%>
  <br>
  <br>
<%end%>